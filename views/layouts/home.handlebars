{{#each r}}
    <div class="col-md-9 panel panel-default" style="margin-left: 14%;">
      <div class="panel-heading">
        <h3 class="panel-title">Patient : {{this.MainDicomTags.PatientID}}</h3>
      </div>
      <div class="panel-body">
        <dl>
            <dt>Patient Name</dt>
            <dd>{{this.MainDicomTags.PatientID}}</dd>
            <dt>Sex</dt>
            <dd>{{this.MainDicomTags.PatientSex}}</dd>
            <dt>IsStable</dt>
            <dd>{{this.IsStable}}</dd>
            <dt>Birth Date</dt>
            <dd>{{this.MainDicomTags.PatientBirthDate}}</dd>
            <dt>Studies</dt>
            <dd><a href="/studies/{{this.Studies}}">{{this.Studies.length}}</a></dd>
        </dl>    
      </div>
    </div>
{{/each}}
